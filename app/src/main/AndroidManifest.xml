<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hu.mystudydemo" >

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ContactActivity"
            android:label="@string/title_activity_contact" />
        <activity
            android:name=".PathTestActivity"
            android:label="@string/title_activity_path_test" />
        <activity
            android:name=".AnimationTestActivity"
            android:label="@string/title_activity_animation_test" />
        <activity
            android:name=".VolleryActivity"
            android:label="@string/title_activity_vollery" />
        <activity
            android:name=".DiskLruCacheActivity"
            android:label="@string/title_activity_disk_lru_cache" >
        </activity>
    </application>

</manifest>
